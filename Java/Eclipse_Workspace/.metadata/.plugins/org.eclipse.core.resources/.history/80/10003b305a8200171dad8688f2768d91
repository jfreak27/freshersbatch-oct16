package com.corejava.collections;

class Person {
	
	private String name;
	private int weight;
	private int height;
	
	Person(String name, int h, int w){

		this.height = h;
		this.weight = w;
		this.name = name;
	}


	
	String getName() { return this.name; }
	int getWeight() { return this.weight; }
	int getHeight() { return this.height; }
}



